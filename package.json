{
  "name": "under-the-hood-app",
  "version": "1.0.0",
  "description": "Everything about front end visualization",
  "main": "index.js",
  "scripts": {
    "dev": "babel-node server-middleware.js",
    "server": "nodemon --exec babel-node server.js",
    "start-cli": "webpack-dev-server --hot --history-api-fallback --config webpack.cli.config.js",
    "start-api": "babel-node server-api.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf ./dist",
    "lint": "eslint .",
    "build-dev": "webpack --mode development ./src/index.js -o ./dist/main-bundle.js",
    "build-prod": "webpack --config webpack.prod.config.js",
    "prod-server": "node ./prod.server.js",
    "build-and-prod-server": "\"npm run clean\" && \"npm run build-prod\" && \"npm run prod-server\""
  },
  "ssr:build": "webpack --mode production --config webpack.ssr.config.js",
  "ssr:start": "node ./server-build/main-bundle.js",
  "author": "Paul Chaysawat",
  "license": "MIT",
